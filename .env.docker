# Docker Environment Configuration
# Copy this to .env and update with your actual values

# OpenAI API Key (REQUIRED for CrewAI agents)
OPENAI_API_KEY=your_openai_api_key_here

# PostgreSQL Configuration
POSTGRES_DB=longquery_demo
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres_secure_password_here

# Application Configuration  
DEBUG=false
LOG_LEVEL=INFO

# Agent Configuration
AGENT_CHECK_INTERVAL=30
AGENT_ENABLED=true

# Security Configuration
SECRET_KEY=your_secret_key_here_please_change_this

# Optional: Monitoring and Alerting
ENABLE_MONITORING=false
GRAFANA_ADMIN_PASSWORD=secure_grafana_password

# Optional: Performance Tuning
MAX_CONNECTIONS=100
WORKER_PROCESSES=4